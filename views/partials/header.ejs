<div class="section">
  <div class="container">
    <div class="navigation" id="navigation">
      <div class="logo">
        <a href="/">Home</a>
      </div>
      <div class="nav-right">
        <img
          src="/images/Nav-Bar-Icon.svg"
          alt="Menu Icon"
          class="menu-icon"
          id="menu-icon"
        />
        <div class="links" id="nav-links">
          <a> <%= user.isAdmin ? 'Admin' : '' %> </a>
          <% if(user.isLoggedIn){%>
           <a>Hallo <%= user.userName%></a> 
           <a href="/logout">Log-Out</a>
          <%}else{%>
            <a href="/login">Log-In</a>
          <%}%>
          <% if(user.isAdmin){%>
            <a href="/newBlog">Neuer Blog</a>
          <%}%>
          <a href="/blog">Blog</a>
          <a href="/register">Registrierung</a>
          <a href="/reserve" class="reserve-button">Reservierung</a>
        </div>
      </div>
    </div>
  </div>
</div>
